local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local plr = Players.LocalPlayer

-- GUI для кнопки
local gui = Instance.new("ScreenGui")
gui.Parent = plr:WaitForChild("PlayerGui")
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 200, 0, 50)
frame.Position = UDim2.new(0.5, -100, 0.5, -25)
frame.BackgroundColor3 = Color3.new(0, 0, 0)
frame.Parent = gui

local spawnBtn = Instance.new("TextButton")
spawnBtn.Size = UDim2.new(1, 0, 1, 0)
spawnBtn.Text = "Spawn All Items"
spawnBtn.BackgroundColor3 = Color3.new(0, 1, 0)
spawnBtn.Parent = frame

-- Список всех предметов (на основе вики и гайдов)
local allItems = {
    -- Ресурсы
    "Logs", "Bolts", "Cultist Gems", "Owl Feather", "Gold Shards", "Rare Metals",
    -- Потребляемые
    "Raw Meat", "Cooked Meat", "Berries", "Carrots", "Chillies", "Bandages", "Morsel",
    -- Оружие и инструменты
    "Old Axe", "Axe", "Pickaxe", "Spear", "Bow", "Gun", "Traps",
    -- Оборудование
    "Old Sack", "Backpack", "Flashlight", "Warm Hats", "Chest",
    -- Структуры (спавн как объекты)
    "Campfire", "Bed", "Walls", "Fences", "Biofuel Processor", "Oil Drill",
    -- Редкие
    "Gem of the Forest", "Meteor Crab Drops"
}

-- Remote Event (замените на актуальное имя, если нужно)
local remoteName = "GiveItem"  -- Или "SpawnItem", "InventoryRemote" и т.д.
local remote = ReplicatedStorage:WaitForChild(remoteName)  -- Если это RemoteEvent

-- Функция спавна
local function spawnAll()
    for _, itemName in pairs(allItems) do
        -- Вызываем Remote Event: FireServer(itemName, количество)
        if remote then
            remote:FireServer(itemName, 10)  -- 10 штук каждого
            print("Spawned: " .. itemName .. " x10")  -- Для отладки в консоли
        else
            warn("RemoteEvent '" .. remoteName .. "' not found! Check ReplicatedStorage.")
        end
        wait(0.1)  -- Задержка, чтобы не спамить сервер
    end
    print("All items spawned!")
end

-- Кнопка активирует спавн
spawnBtn.MouseButton1Click:Connect(spawnAll)
